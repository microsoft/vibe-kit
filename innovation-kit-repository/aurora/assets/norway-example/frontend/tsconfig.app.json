{
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ES2020",
    "strict": true,
    "baseUrl": "./",
    "types": ["vite/client"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
