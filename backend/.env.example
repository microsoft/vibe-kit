# Azure AI Foundry endpoint for model inference
FOUNDRY_ENDPOINT=

# Azure AI Foundry Primary Key (see Authentication type: Key in Azure portal)
FOUNDRY_KEY=

# Azure Blob Storage (Identity-based authentication - no SAS needed)
# Storage account name from Azure portal
AZURE_STORAGE_ACCOUNT_NAME=

# Container name for blob storage
AZURE_STORAGE_CONTAINER_NAME=

# Optional: Hugging Face cache location for model checkpoints (~500 MB)
HF_HOME=
